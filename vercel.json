{
  "version": 2,
  "buildCommand": "cd frontend/my-book && npm install && npm run build",
  "outputDirectory": "frontend/my-book/build",
  "installCommand": "npm install --prefix frontend/my-book",
  "framework": null,
  "public": false,
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/docs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
